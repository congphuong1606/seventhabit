<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:font="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="5dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/border_card"
        android:orientation="vertical">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="@drawable/mind_card_11_1" />

        <ScrollView
            android:id="@+id/scroll_11"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="15dp">

                <com.discovertodo.phone.android.util.TextViewWithFont
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="@string/text_card_11_1"
                    font:name="MSGothic"
                    font:textcolor="blue"
                    font:textsize="nomarlxx" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="D"
                                font:name="MSGothic"
                                font:textcolor="blue"
                                font:textsize="nomarl" />

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/text_card_11_2"
                                font:name="MSGothic"
                                font:textcolor="black"
                                font:textsize="nomarl" />

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/text_card_11_3"
                                font:name="MSGothic"
                                font:textcolor="blue"
                                font:textsize="nomarl" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="3dp">

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="O"
                                font:name="MSGothic"
                                font:textcolor="blue"
                                font:textsize="nomarl" />

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/text_card_11_4"
                                font:name="MSGothic"
                                font:textcolor="black"
                                font:textsize="nomarl" />

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/text_card_11_5"
                                font:name="MSGothic"
                                font:textcolor="blue"
                                font:textsize="nomarl" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="3dp">

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="O"
                                font:name="MSGothic"
                                font:textcolor="blue"
                                font:textsize="nomarl" />

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/text_card_11_6"
                                font:name="MSGothic"
                                font:textcolor="black"
                                font:textsize="nomarl" />

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/text_card_11_7"
                                font:name="MSGothic"
                                font:textcolor="blue"
                                font:textsize="nomarl" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="3dp">

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="R"
                                font:name="MSGothic"
                                font:textcolor="blue"
                                font:textsize="nomarl" />

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/text_card_11_8"
                                font:name="MSGothic"
                                font:textcolor="black"
                                font:textsize="nomarl" />

                            <com.discovertodo.phone.android.util.TextViewWithFont
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/text_card_11_9"
                                font:name="MSGothic"
                                font:textcolor="blue"
                                font:textsize="nomarl" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/img_card_11_1"
                            android:layout_width="160dp"
                            android:layout_height="50dp"
                            android:layout_gravity="right"
                            android:layout_marginTop="15dp"
                            android:background="@drawable/mind_card_11_1_1" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom">

                        <ImageView
                            android:id="@+id/img_card_11_2"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_marginTop="15dp"
                            android:background="@drawable/mind_card_11_1_2" />
                    </LinearLayout>

                </LinearLayout>

                <com.discovertodo.phone.android.util.TextViewWithFont
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="25dp"
                    android:text="@string/text_card_11_10"
                    font:name="MSGothic"
                    font:textcolor="black"
                    font:textsize="nomarl" />

                <com.discovertodo.phone.android.util.TextViewWithFont
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:text="@string/text_card_11_11"
                    font:name="MSGothic"
                    font:textcolor="blue"
                    font:textsize="nomarlxx" />

                <com.discovertodo.phone.android.util.TextViewWithFont
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="3dp"
                    android:text="@string/text_card_11"
                    font:name="MSGothic"
                    font:textcolor="black"
                    font:textsize="nomarl" />

                <com.discovertodo.phone.android.util.TextViewWithFont
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:text="@string/text_card_11_12"
                    font:name="MSGothic"
                    font:textcolor="blue"
                    font:textsize="nomarlxx" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="3dp"
                    android:orientation="horizontal">

                    <com.discovertodo.phone.android.util.TextViewWithFont
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/text_card_9_25"
                        font:name="MSGothic"
                        font:textcolor="black"
                        font:textsize="nomarl" />

                    <com.discovertodo.phone.android.util.TextViewWithFont
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/text_card_11_13"
                        font:name="MSGothic"
                        font:textcolor="black"
                        font:textsize="nomarl" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="3dp"
                    android:orientation="horizontal">

                    <com.discovertodo.phone.android.util.TextViewWithFont
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/text_card_9_25"
                        font:name="MSGothic"
                        font:textcolor="black"
                        font:textsize="nomarl" />

                    <com.discovertodo.phone.android.util.TextViewWithFont
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/text_card_11_14"
                        font:name="MSGothic"
                        font:textcolor="black"
                        font:textsize="nomarl" />
                </LinearLayout>


            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</LinearLayout>